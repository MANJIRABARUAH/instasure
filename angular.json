{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "tournament-app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/tournament-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/vendor/datatables/dataTables.bootstrap4.css",
              "src/vendor/fontawesome-free/css/all.css"
            ],
            "scripts": [
              "./src/vendor/jquery/jquery.min.js",
              "./src/vendor/bootstrap/js/bootstrap.bundle.min.js",
              "./src/vendor/fontawesome-free/js/all.js",
              "./src/vendor/jquery-easing/jquery.easing.min.js",
              "./src/vendor/chart.js/Chart.min.js",
              "./src/vendor/datatables/jquery.dataTables.js",
              "./src/vendor/datatables/dataTables.bootstrap4.js",
              "./src/js/sb-admin.js",
              "./src/js/demo/chart-area-demo.js",
              "./src/js/demo/chart-bar-demo.js",
              "./src/js/demo/chart-pie-demo.js",
              "./src/js/demo/datatables-demo.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "tournament-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "tournament-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tournament-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/vendor/fontawesome-free/webfonts/fa-regular-400.ttf",
              "src/vendor/fontawesome-free/webfonts/fa-brands-400.ttf",
              "src/vendor/fontawesome-free/webfonts/fa-solid-900.ttf"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "tournament-app-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "tournament-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tournament-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "tournament-app"
}